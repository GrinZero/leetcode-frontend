{
  "private": true,
  "name": "dumi-app",
  "scripts": {
    "start": "TAILWIND_MODE=watch dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "dumi": "^1.0.13",
    "lint-staged": "^10.0.7",
    "postcss-import": "^11",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tdesign-react": "^0.41.1"
  }
}
